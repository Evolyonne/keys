<h1>Liste des biens</h1>

<!-- <div id="depenses" data-depenses= <%=@depenses %>></div> -->
<div class="row justify-content-around">
  <div class="card" class="col-6">
      <div class="card-body" style="width: 350px;">
          <canvas id="myChartCourbe"></canvas>
      </div>
  </div>
  <div class="card" class="col-6">
      <div class="card-body" style="width: 350px;">
          <canvas id="myChartBiens"></canvas>
      </div>
  </div>
</div>
<!-- <canvas id="myChart" width="400" height="400"></canvas> -->

<div class="container">
  <div class="row">
    <div class="col 6">
      <ul>
          <% @biens.each do |bien| %>
              <li>
                  <%= link_to bien.nom, bien_path(bien) %>
              </li>
          <% end %>
      </ul>
    </div>
    <div class="col 6" id="map"
         style="width: 100%; height: 600px;"
         data-markers="<%= @markers.to_json %>"
         data-mapbox-api-key="<%= ENV['MAPBOX_API_KEY'] %>">
  </div>
</div>
<div class="">
  <div class="d-flex justify-content-around align-items-center">
    <div>
      <%= link_to "Ajoutez un nouveau bien ğŸš€", new_bien_path%>
    </div>
